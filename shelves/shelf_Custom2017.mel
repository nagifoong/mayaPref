        -command "import maya.cmds as cmds\ndef asg(n,m):##n=cmds.ls(sl=1)\n    for q in n:\n        if m == -1 :\n            cmds.setAttr(q+\".overrideEnabled\",0)\n        else:\n            itemAttr = cmds.getAttr(q+\".overrideEnabled\")\n            if itemAttr == 0:cmds.setAttr(q+\".overrideEnabled\",1)\n            cmds.setAttr(q+\".overrideColor\",m-1)\n\nif cmds.window('winColor',q=1,ex=1):\n        cmds.deleteUI('winColor')\n\ncmds.window(\"winColor\",t=\"Color Object(s)\",vis=1,s=1)\ncmds.formLayout('clrForm',nd=1)\ncmds.gridLayout('btGrid',cr=1,cw=145,nrc=(1,1),p='clrForm',aec=0)#cmds.gridLayout('btGrid',q=1,nc=1)\ncmds.colorIndexSliderGrp( \"sldC\",label='Select Color', min=0, max=20, value=10 ,p='clrForm',adj=3,cw3=(60,50,100))##len(cmds.ls(sl=1))\ncmds.button ('asBt',l=\"Assign to Selected Object(s)\",c=\"asg(cmds.ls(sl=1),cmds.colorIndexSliderGrp( 'sldC',q=1,value=1 ))\",h=30,p='clrForm')\n#cmds.grabColor()#cmds.colorIndexSliderGrp( 'sldC',q=1,value=1 )  \ncmds.button('btBlue',l='Blue',c=\"asg(cmds.ls(sl=1),7)\",bgc=[0,0,1],p='btGrid')\ncmds.button('btPink',l='Pink',c=\"asg(cmds.ls(sl=1),10)\",bgc=[0.78431373834609985, 0.0, 0.78431373834609985],p='btGrid')\ncmds.button('btRed',l='Red',c=\"asg(cmds.ls(sl=1),14)\",bgc=[1,0,0],p='btGrid')\ncmds.button('btGreen',l='Green',c=\"asg(cmds.ls(sl=1),15)\",bgc=[0,1,0],p='btGrid')\ncmds.button('btWhite',l='White',c=\"asg(cmds.ls(sl=1),17)\",bgc=[1,1,1],p='btGrid')\ncmds.button('btYellow',l='Yellow',c=\"asg(cmds.ls(sl=1),18)\",bgc=[1,1,0],p='btGrid')\ncmds.button('btLightBlue',l='LightBlue',c=\"asg(cmds.ls(sl=1),19)\",bgc=[0.39215686917304993, 0.86274510622024536, 1.0],p='btGrid')\ncmds.button('btOff',l='Disable',c=\"asg(cmds.ls(sl=1),-1)\",bgc=[.2, .2,.2],p='btGrid')\n\ncmds.formLayout('clrForm',e=1,ac=[('sldC','right',5,'asBt'),('btGrid','top',20,'sldC')],\n                af=[('sldC','left',5),('sldC','top',5),\n                ('asBt','right',5),('asBt','top',1),\n                ('btGrid','left',5),('btGrid','right',5),('btGrid','bottom',1)])" 
