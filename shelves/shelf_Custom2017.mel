        -command "for q in cmds.ls(sl=1):\n    meshName = \"edge_\"+q.split(\"[\")[-1][:-1]\n    crvList =cmds.duplicateCurve(q,ch=1,name = meshName)\n    cmds.rename(crvList[1],meshName+\"_crvMeshEdge\")\n    cvShape = cmds.listRelatives(crvList[0],type=\"nurbsCurve\")[0]\n    pointInfo = cmds.shadingNode(\"pointOnCurveInfo\",au=1,name = \"%s_pointOnCurveInfo\"%meshName)\n    plusNode = cmds.shadingNode(\"plusMinusAverage\",au=1,name = \"%s_plusMinusAverage\"%meshName)\n    posGrp = cmds.group(em=1,name = \"CTRL_%s_pos_grp\"%meshName)\n    offGrp = cmds.group(parent=posGrp,em=1,name = \"CTRL_%s_offset_grp\"%meshName)\n    subGrp = cmds.group(parent=offGrp,em=1,name = \"CTRL_%s_sub_grp\"%meshName)\n    cir = cmds.circle(c=(0,0,0) ,nr=(0,1,0) ,sw=360 ,r=1 ,d=3 ,ut=0 ,tol=0.01 ,s=8 ,ch=0,name = \"CTRL_%s\"%meshName)[0]\n    \n    cmds.addAttr(posGrp,longName = \"parameter\",min = 0,max=100,keyable=1,at='double')\n    ucNode = cmds.shadingNode(\"unitConversion\",au=1,name = \"%s_uc\"%pointInfo)\n    cmds.setAttr(\"%s.conversionFactor\"%ucNode,.01)\n    cmds.connectAttr(\"%s.parameter\"%posGrp,\"%s.input\"%ucNode)\n    cmds.connectAttr(\"%s.output\"%ucNode,\"%s.parameter\"%pointInfo)\n    \n    \n    cmds.parent(cir,subGrp)\n    cmds.setAttr(\"%s.operation\"%plusNode,2)\n    cmds.setAttr(\"%s.turnOnPercentage\"%pointInfo,1)\n    cmds.connectAttr(\"%s.worldSpace[0]\"%cvShape,\"%s.inputCurve\"%pointInfo)\n    cmds.connectAttr(\"%s.position\"%pointInfo,\"%s.translate\"%posGrp)\n    cmds.connectAttr(\"%s.translate\"%cir,\"%s.input3D[0]\"%plusNode)\n    cmds.disconnectAttr(\"%s.translate\"%cir,\"%s.input3D[0]\"%plusNode)\n    cmds.connectAttr(\"%s.translate\"%cir,\"%s.input3D[1]\"%plusNode)\n    cmds.connectAttr(\"%s.output3D\"%plusNode,\"%s.translate\"%subGrp)\n    \n    \n    " 
        -sourceType "python" 
        -commandRepeatable 1
        -flat 1
        -mi "-------------" ( "print(\"User defined menuItem\");" )
        -mi "change shape" ( "selectionList = cmds.ls(sl=1)[:-1]\nnewCv = cmds.ls(sl=1)[-1]\n\nfor sel in selectionList:\n    dulCv = cmds.duplicate(newCv,rc=1)[0]\n    cmds.parentConstraint(sel,dulCv,mo=0,name = \"PCCC1\")\n    cmds.delete(\"PCCC1\")\n    cmds.parent(dulCv,sel)\n    cmds.makeIdentity(apply = True, translate = True)\n    cvName = cmds.listRelatives(sel,c=1,type=\"nurbsCurve\")[0]\n    cmds.delete (cmds.listRelatives(sel,c=1,type=\"nurbsCurve\"))\n    newCurve = cmds.parent(cmds.listRelatives(dulCv,c=1,f=1,type=\"nurbsCurve\"),sel,r=1,s=1)\n    cmds.rename(newCurve,cvName)\n    cmds.delete(dulCv)\ncmds.delete(newCv)\ncmds.select(selectionList)" )
