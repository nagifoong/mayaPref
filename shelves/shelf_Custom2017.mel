        -command "import maya.cmds as cmds\nimport os,sys\ndef replaceString(n):\n    n.replace(\"\\\\\",'/')\n    if n[0] == \"\\\"\":n=n[1:-1]    \n    return n \ndef oP(n):     \n    cmds.deleteUI('windowOp')   \n    cmds.file (replaceString(n),f=1 ,ignoreVersion=1  ,o=1)  \n    \ndef iM(n):\n    cmds.deleteUI('windowOp')   \n    cmds.file (replaceString(n),i=1,options=\"mo=1\"  ,ignoreVersion=1 ,mnc=0,pr=1)     \n    \ndef txtScroll(n):\n    if \".ma\" not in n:\n        fileLoc = \"Q:/Production/Assets/3D_Assets/Characters/Global/\" +n+'/KAZ_C_'+n+'_PRD.ma'        \n    else:\n        fileLoc = \"Q:/Production/Assets/3D_Assets/Characters/Global/\" +n\n    cmds.textFieldGrp('tfOpn',e=1,tx=fileLoc) \ndef checkLoc(n):\n    fileLoc = cmds.textFieldGrp('tfOpn',q=1,tx=1)\n    if fileLoc[-1] == \"\\\"\":fileLoc=fileLoc[:-1]\n    if fileLoc[0] == \"\\\"\":fileLoc=fileLoc[1:]\n    try:\n        fileType = cmds.file(fileLoc,q=1,typ=1)[0]\n        \n    except:\n        cmds.iconTextButton('iTB',e=1,image1='muted.png', label='File is not existed.')\n        cmds.button('btOpen',e=1,en=0,bgc=[1, 0, 0])\n        cmds.button('btImport',e=1,en=0)\n        sys.exit()\n    if fileType != 'directory':\n        if os.path.exists(fileLoc):\n            cmds.iconTextButton('iTB',e=1,image1='unmuted.png', label='Location existed.')\n            cmds.button('btOpen',e=1,en=1,bgc=[0,1,0])\n            cmds.button('btImport',e=1,en=1)\n        else:\n            cmds.iconTextButton('iTB',e=1,image1='muted.png', label='Location not existed.')\n            cmds.button('btOpen',e=1,en=0,bgc=[1, 0, 0])\n            cmds.button('btImport',e=1,en=0)\n    else:\n        cmds.iconTextButton('iTB',e=1,image1='muted.png', label='Filetype missing.')\n        cmds.button('btOpen',e=1,en=0,bgc=[1, 0, 0])\n        cmds.button('btImport',e=1,en=0)\n        \nloc = \"/\".join(r\"Q:\\Production\\Assets\\3D_Assets\\Characters\\Global\".split(\"\\\\\"))\nfileList = sorted(os.listdir(loc))\nfileList = [fi for fi in fileList if fi[0]!=\".\"]\n\nif cmds.window('windowOp',q=1,ex=1):\n    cmds.deleteUI('windowOp')\nwindow = cmds.window('windowOp',t='Open File',wh=(200,50),vis=1,s=1)\ncmds.window('windowOp',q=1,wh=1)\ncmds.formLayout('opForm',nd=100,p='windowOp')\n\ncmds.textFieldGrp('tfOpn',adj=2,l=\"File Location:\",cl2=[\"left\",\"left\"],cw2=[70,130],p='opForm',tcc=checkLoc)\n\ntxtItem = cmds.textScrollList(ams=0,\n                            append = fileList,\n                            sc=\"txtScroll(cmds.textScrollList(txtItem,q=1,si=1)[0])\",\n                            p=\"opForm\")\n\ncmds.gridLayout(\"opGrid\",nc=3,cw=110,p=\"opForm\",cr=1)\ncmds.button('btOpen',l=\"Open\",bgc=[1, 0, 0],c=\"oP(cmds.textFieldGrp('tfOpn',q=1,tx=1) )\",p=\"opGrid\",en=0)\ncmds.button('btImport',l=\"Import\",c=\"iM(cmds.textFieldGrp('tfOpn',q=1,tx=1) )\",p=\"opGrid\",en=0)\ncmds.iconTextButton('iTB', style='iconAndTextVertical', image1='muted.png' , label='Textfield is empty.',p =\"opGrid\")\n\ncmds.formLayout(\"opForm\",e=1,ac=[(txtItem,'top',5,'tfOpn'),(\"opGrid\",'top',5,txtItem)],\n                af=[('tfOpn','top',5),('tfOpn','left',5),('tfOpn','right',5),\n                    (txtItem,'top',5),(txtItem,'left',5),(txtItem,'right',5),(txtItem,'bottom',45),\n                    (\"opGrid\",'bottom',5),(\"opGrid\",'left',5),(\"opGrid\",'right',5)])\ncmds.setFocus('tfOpn')" 
