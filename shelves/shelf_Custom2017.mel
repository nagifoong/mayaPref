        -command "#Please select an ikHandle and a controller which will be assigned additional attribute(optional).##\n\nimport maya.cmds as cmds\nimport sys\ndef createSphere (name):\n    cA=cmds.circle(c=[0,0,0],nr=[0,1,0],sw=360,r=1,d=3,ut=0,tol=.01,s=8,ch=0,n=name)\n    cB=cmds.circle(c=[0,0,0],nr=[1,0,0],sw=360,r=1,d=3,ut=0,tol=.01,s=8,ch=0)\n    cC=cmds.circle(c=[0,0,0],nr=[0,0,1],sw=360,r=1,d=3,ut=0,tol=.01,s=8,ch=0)    \n    cmds.parent(cmds.listRelatives(cC[0],c=1)[0],cmds.listRelatives(cB[0],c=1)[0],cA[0],r=1,s=1)\n    cmds.delete(cB,cC)\n    cmds.select(cl=1)\n    return cA\n\ndef createStretchyIK(*arg):           \n    tempMain = arg[0]\n        \n    if (len(tempMain)>2 or len(tempMain)<1) or cmds.objectType(tempMain[0],isType=\"ikHandle\")==False :\n        cmds.warning(\"Please select an ikHandle and a controller which will be assigned additional attribute(optional).\")    \n        sys.exit()\n    else:\n        handle=tempMain[0]\n        ikName=cmds.ikHandle(handle,q=1,n=1)\n        jntList=cmds.ikHandle(handle,q=1,jl=1)[1:]\n        tempList = cmds.listRelatives(jntList[-1],c=1)\n        for temp in tempList:\n            if temp != cmds.ikHandle(handle,q=1,ee=1):jntList.append(temp)\n            else: break \n        \n        startJnt=cmds.xform(cmds.ikHandle(handle,q=1,jl=1)[0] ,q=1,t=1,ws=1)\n        endJnt = cmds.xform(jntList[-1],q=1,t=1,ws=1)\n        locA= cmds.spaceLocator(n=ikName+\"_stretch_start_LOC\")\n        cmds.setAttr(locA[0]+\".translate\",startJnt[0],startJnt[1],startJnt[2],typ=\"double3\")\n        locB=cmds.spaceLocator(n=ikName+\"_stretch_end_LOC\")\n        cmds.setAttr(locB[0]+\".translate\",endJnt[0],endJnt[1],endJnt[2],typ=\"double3\")\n        disParent=cmds.createNode(\"transform\",n=ikName+\"_distance\")\n        disNode=cmds.createNode(\"distanceDimShape\",n=ikName+\"_distanceShape\",p=disParent)\n        cmds.connectAttr(locA[0]+\"Shape.worldPosition\",disNode+\".startPoint\",f=1)\n        cmds.connectAttr(locB[0]+\"Shape.worldPosition\",disNode+\".endPoint\",f=1)\n        \n        totalDis = sum([cmds.getAttr(jnt+\".tx\") for jnt in jntList])\n        \n        if len(tempMain)==1:\n            newCtrl = createSphere(\"CTRL_\"+ikName)[0]\n            cmds.addAttr(newCtrl,ln=\"stretchy\",at=\"double\",min=0,max=1,dv=1,k=1)\n            \n        elif len(tempMain)==2:\n            newCtrl = tempMain[1]\n            cmds.addAttr(newCtrl,ln=\"stretchy\",at=\"double\",min=0,max=1,dv=1,k=1)    \n            \n        \n        switchNode1 = cmds.shadingNode(\"multiplyDivide\",au=1,n=ikName+\"_stretch_Switch\")\n        scaleNode = cmds.shadingNode(\"multiplyDivide\",au=1,n=ikName+\"_stretch_scaleFix\")\n        \n        cmds.setAttr(scaleNode+'.operation',2)\n        cmds.connectAttr(disNode+\".distance\",switchNode1+\".input1X\")\n        cmds.connectAttr(switchNode1+\".outputX\",scaleNode+\".input1X\")\n        cmds.connectAttr (newCtrl+\".stretchy\",switchNode1+\".input2X\")\n            \n        for jnt in jntList:\n            cmds.setDrivenKeyframe(jnt+\".translateX\",cd=disNode+\".distance\",dv=totalDis,v=(cmds.getAttr(jnt+\".translateX\")),itt=\"linear\",ott=\"linear\")\n            cmds.setDrivenKeyframe(jnt+\".translateX\",cd=disNode+\".distance\",dv=totalDis*2,v=(cmds.getAttr(jnt+\".translateX\")*2),itt=\"linear\",ott=\"linear\")\n            animCurv=cmds.listConnections(jnt+\".translateX\",t=\"animCurve\")[0]\n            cmds.setAttr(animCurv+\".postInfinity\",4)    \n            cmds.connectAttr(scaleNode+\".outputX\",animCurv+\".input\",f=1)\n        distanceGrp = cmds.group(em=1,n=ikName + \"_distance_util_grp\")\n        cmds.parent(locA,locB,disNode,distanceGrp)        \n        cmds.parentConstraint (cmds.ikHandle(handle,q=1,jl=1)[0],locA,mo=1)\n        #handle=cmds.ls(sl=1)\n        handleParent = cmds.listRelatives(handle,p=1)\n        if handleParent==None :\n            cmds.warning(\"Please manually parent \"+locB[0]+\".\")\n            sys.exit()\n\n        else:\n            cmds.parentConstraint (handleParent[0],locB,mo=1)\n        #cmds.parentConstraint (handle,locB,mo=1)\n\ncreateStretchyIK(cmds.ls(sl=1))\n" 
        -sourceType "python" 
        -commandRepeatable 1
        -flat 1
