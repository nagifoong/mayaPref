        -command "//CREATE FK/IK LIMBS///\nstring $listSkin[] = `ls -sl`;\nduplicate -rc $listSkin[0];duplicate -rc $listSkin[0];\n\n\nint $size = size($listSkin);\n$nameIK = $listSkin[0]+\"1\";\nselect $nameIK;\nsearchReplaceNames \"skin1\" \"IK\" \"hierarchy\";\n$nameFK = $listSkin[0]+\"2\";\nselect $nameFK;\nsearchReplaceNames \"skin2\" \"FK\" \"hierarchy\";\n$nameIK = substituteAllString($nameIK,\"skin1\",\"IK\");\n$nameFK = substituteAllString($nameFK ,\"skin2\",\"FK\");\nselect -hi $nameIK;\nstring $listIK[] = `ls -sl`;\nselect -hi $nameFK;\nstring $listFK[] = `ls -sl`;\n\n\n//CREATE NODE//\nfor ($item in $listIK){\n    $temp1 = substituteAllString($item,\"_IK\",\"_rot\");\n    $temp2 = substituteAllString($item,\"_IK\",\"_trans\");\n    $temp3 = substituteAllString($item,\"_IK\",\"_scale\");    \n    shadingNode -asUtility blendColors -n $temp1;\n    shadingNode -asUtility blendColors -n $temp2;\n    shadingNode -asUtility blendColors -n $temp3;  \n}\n//CONNECT ATTRIBUTE//print $listIK\nfor ($item in $listIK){\n    $itemR = $item + \".rotate\";\n    $itemT = $item + \".translate\";\n    $itemS = $item + \".scale\";\n    \n    $tempR = substituteAllString($item,\"IK\",\"rot\");\n    $tempR = $tempR + \".color2\";    \n    $tempT = substituteAllString($item,\"IK\",\"trans\");\n    $tempT = $tempT + \".color2\";\n    $tempS = substituteAllString($item,\"IK\",\"scale\");\n    $tempS = $tempS + \".color2\";    \n    \n    connectAttr -f $itemT $tempT;\n    connectAttr -f $itemR $tempR;\n    connectAttr -f $itemS $tempS;\n}\n\nfor ($item in $listFK){    \n    $itemR = $item + \".rotate\";\n    $itemT = $item + \".translate\";\n    $itemS = $item + \".scale\";\n    \n    $tempR = substituteAllString($item,\"FK\",\"rot\");\n    $tempR = $tempR + \".color1\";    \n    $tempT = substituteAllString($item,\"FK\",\"trans\");\n    $tempT = $tempT + \".color1\";    \n    $tempS = substituteAllString($item,\"FK\",\"scale\");\n    $tempS = $tempS + \".color1\"; \n    \n    connectAttr -f $itemT $tempT ;\n    connectAttr -f $itemR $tempR;\n    connectAttr -f $itemS $tempS;\n}\n\nfor ($item in $listFK){\n    $item1 = substituteAllString($item,\"FK\",\"skin\");\n    $itemR = $item1 + \".rotate\";\n    $itemT = $item1 + \".translate\";\n    $itemS = $item1 + \".scale\";\n    \n    $tempR = substituteAllString($item,\"FK\",\"rot\");\n    $tempR = $tempR + \".output\";    \n    $tempT = substituteAllString($item,\"FK\",\"trans\");\n    $tempT = $tempT + \".output\";    \n    $tempS = substituteAllString($item,\"FK\",\"scale\");\n    $tempS = $tempS + \".output\"; \n    \n    connectAttr -f $tempT $itemT;\n    connectAttr -f $tempR $itemR;\n    connectAttr -f $tempS $itemS;\n}\n\n//FINISHING//\n\n$grpName = substituteAllString($listSkin[0],\"skin\",\"util_grp\");\n\n\ngroup -em -n $grpName;\naddAttr -ln \"ikFk\"  -at double  -min 0 -max 1 -k 1 $grpName;\n\n\n//parentConstraint -n \"pcTemp1\"$listSkin[1] $grpName ;delete pcTemp1;\n//parent $listFK[0] $listIK[0] $grpName ;\n\n$fkGRP = `group -em -n($listFK[0] + \"_grp\")`;\nparentConstraint -n \"pcTemp1\" $listSkin[1] $fkGRP ;delete pcTemp1;\nparent $listFK[0] $fkGRP ;\n\n$ikGRP = `group -em -n($listIK[0] + \"_grp\")`;\nparentConstraint -n \"pcTemp1\" $listSkin[1] $ikGRP ;delete pcTemp1;\nparent $listIK[0] $ikGRP ;\n\nparent $ikGRP $fkGRP $grpName;\n\n$groupConn = $grpName + \".ikFk\";\n\nfor ($item in $listFK){    \n    $temp1 = substituteAllString($item,\"FK\",\"rot\");\n    $tempR = $temp1 + \".blender\";    \n    $temp2 = substituteAllString($item,\"FK\",\"trans\");\n    $tempT = $temp2 + \".blender\";    \n    $temp3 = substituteAllString($item,\"FK\",\"scale\");\n    $tempS = $temp3 + \".blender\";    \n    \n    connectAttr -f $groupConn $tempR ;\n    connectAttr -f $groupConn $tempT ;\n    connectAttr -f $groupConn $tempS ;\n}\n\n$revName = substituteAllString($listSkin[0],\"skin\",\"vis_REVERSE\");\nshadingNode -asUtility reverse -n $revName;\nconnectAttr -f $groupConn ($revName+\".inputX\") ;\nconnectAttr -f ($revName+\".inputX\") ($fkGRP+\".visibility\") ;\nconnectAttr -f ($revName+\".outputX\") ($ikGRP+\".visibility\")  ;\n//connectAttr -f ($listSkin[1]+\".translate\") ($grpName+\".translate\" );\n//connectAttr -f ($listSkin[1]+\".rotate\") ($grpName+\".rotate\" );\n\n\n//LOCATOR CREATION//\n$locName = substituteAllString($listSkin[1],\"skin\",\"LOC\");\nspaceLocator -p 0 0 0 -n $locName;\ngroup -n ($locName + \"_grp\") $locName;\nparentConstraint $listSkin[1] ($locName + \"_grp\");\nparentConstraint -n\"loc_pC1\" $listSkin[0] $locName;delete loc_pC1;\nparentConstraint -mo $listSkin[1] $fkGRP; \nparentConstraint -mo $listSkin[1] $ikGRP;\nparent ($locName + \"_grp\") $grpName;" 
